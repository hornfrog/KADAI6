<table>
 <thead>
  <tr>
   <th>ID</th>
   <th>タイトル</th>
   <th>開始日</th> 
   <th>終了日</th>
   <th>終日</th>
   <th>更新日時</th>
  </tr>
</thead>

<tbody>
 <% @users.each do |user| %>
  <tr>
   <td><%= user.id %></td>
   <td><%= user.title %></td>
   <td><%= user.start %></td>
   <td><%= user.end %></td>
   <td><%= user.updated_at %></td>
  </tr>
<% end %>
 </tbody>
</table>